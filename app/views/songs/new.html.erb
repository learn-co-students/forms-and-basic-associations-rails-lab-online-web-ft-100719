<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
<%= f.label :artist_name %>
<%= f. text_field :artist_name%>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :genre %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  <%= f.fields_for :notes do |notes_builder|%>
  <%= notes_builder.label :notes%>
  <%= notes_builder.text_area :content%>
  
  <br>
  <%= f.submit %>
  <%end%>
<% end %>


